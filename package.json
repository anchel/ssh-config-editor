{
  "name": "ssh-config-editor",
  "version": "1.0.0",
  "main": "dist-electron/main/index.js",
  "description": "SSH Config Editor",
  "author": "anchel",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "package": "vue-tsc --noEmit && vite build && electron-forge make",
    "make": "vue-tsc --noEmit && vite build && electron-forge make"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.67",
    "@electron-forge/maker-deb": "^6.0.0-beta.67",
    "@electron-forge/maker-rpm": "^6.0.0-beta.67",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.67",
    "@electron-forge/maker-zip": "^6.0.0-beta.67",
    "@vitejs/plugin-vue": "^3.1.2",
    "electron": "^21.1.0",
    "less": "^4.1.3",
    "typescript": "^4.8.4",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.1.4",
    "vite-electron-plugin": "^0.4.4",
    "vite-plugin-electron-renderer": "^0.10.0",
    "vue": "^3.2.40",
    "vue-tsc": "^1.0.6"
  },
  "debug": {
    "env": {
      "VITE_DEV_SERVER_URL": "http://127.0.0.1:3344"
    }
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ],
  "dependencies": {
    "@ant-design/icons-vue": "^6.1.0",
    "@codemirror/theme-one-dark": "^6.1.0",
    "ant-design-vue": "^3.2.13",
    "codemirror": "^6.0.1",
    "electron-squirrel-startup": "^1.0.0",
    "monaco-editor": "^0.34.0",
    "readdirp": "^3.6.0",
    "ssh-config": "^4.1.6",
    "ssh-keygen-lite": "^1.3.0",
    "vue-codemirror": "^6.1.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "ssh_config_editor"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
